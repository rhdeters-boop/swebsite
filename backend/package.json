{
  "name": "void-of-desire-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend API for Void of Desire creator platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest -c jest.config.esm.js",
    "test:watch": "jest -c jest.config.esm.js --watch",
    "test:changed": "jest -c jest.config.esm.js --onlyChanged",
    "test:unit": "jest -c jest.config.esm.js --testPathPattern='__tests__|tests/unit|services/__tests__|utils/__tests__|middleware/__tests__' --runInBand",
    "test:api": "jest -c jest.config.esm.js --testPathPattern='tests/api' --runInBand",
    "test:contract": "jest -c jest.config.esm.js --testPathPattern='tests/contract' --runInBand",
    "test:coverage": "COVERAGE=1 jest -c jest.config.esm.js --coverage",
    "test:containers": "JEST_INTEGRATION_MODE=testcontainers jest -c jest.config.esm.js --testPathPattern='tests/api|tests/contract' --runInBand",
    "test:storage": "node scripts/testStorageConfig.js",
    "migrate:azure": "node scripts/migrateToAzureStorage.js",
    "seed:creators": "node scripts/seedCreators.js",
    "create:media": "node scripts/createSampleMedia.js",
    "lint": "eslint . --ext .ts,.js",
    "typecheck": "tsc -p tsconfig.testing.json --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@aws-sdk/s3-presigned-post": "^3.859.0",
    "@aws-sdk/s3-request-presigner": "^3.859.0",
    "@azure/storage-blob": "^12.28.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.5",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "stripe": "^13.6.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@faker-js/faker": "^9.9.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/multer": "^1.4.7",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "nock": "^14.0.9",
    "nodemon": "^3.0.1",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "testcontainers": "^11.5.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "zod": "^4.0.15"
  },
  "keywords": [
    "subscription",
    "media",
    "premium",
    "payments",
    "stripe"
  ],
  "author": "",
  "license": "MIT"
}
